      </attDef><attDef ident="to" usage="opt">
        <desc>Specifies the end-point of the location in a normalized form.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.WORD"/>
        </datatype>
      </attDef></attList><remarks xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
