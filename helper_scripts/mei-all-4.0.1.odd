          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BOOLEAN"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.halfmRpt.log" module="MEI.cmn" type="atts">
