      <memberOf key="model.paracontentPart"/>
    </classes>
  </classSpec><elementSpec rend="add" ident="sp" module="MEI.drama"><desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">(speech) – Contains an individual speech in a performance text.</desc><classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <memberOf key="att.common"/>
      <memberOf key="att.facsimile"/>
      <memberOf key="att.lang"/>
      <memberOf key="att.sp.anl"/>
      <memberOf key="att.sp.ges"/>
      <memberOf key="att.sp.log"/>
      <memberOf key="att.sp.vis"/>
