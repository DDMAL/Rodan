      <attDef ident="c" usage="opt">
        <desc>Indicates the pedal setting for the harp's C strings.</desc>
        <defaultVal>n</defaultVal>
        <valList type="closed">
          <valItem ident="f">
            <desc>Flat.</desc>
          </valItem>
          <valItem ident="n">
            <desc>Natural.</desc>
          </valItem>
          <valItem ident="s">
            <desc>Sharp.</desc>
          </valItem>
        </valList>
      </attDef>
      <attDef ident="d" usage="opt">
        <desc>Indicates the pedal setting for the harp's D strings.</desc>
