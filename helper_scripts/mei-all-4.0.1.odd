      <optional xmlns="http://relaxng.org/ns/structure/1.0"><ref name="model.backLike"/></optional>
    </content>
  </macroSpec><macroSpec rend="add" ident="macro.struc-unstrucContent" module="MEI.shared" type="pe">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">Provides a choice between structured and unstructured/mixed content.</desc>
    <content xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <choice xmlns="http://relaxng.org/ns/structure/1.0"><group><zeroOrMore><ref name="model.headLike"/></zeroOrMore><zeroOrMore><ref name="model.pLike"/></zeroOrMore></group><zeroOrMore><choice><rng:text xmlns="http://www.tei-c.org/ns/1.0"/><ref name="model.textPhraseLike.limited"/></choice></zeroOrMore></choice>
    </content>
  </macroSpec><macroSpec rend="add" ident="macro.titlePart" module="MEI.shared" type="pe">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">Groups elements that may appear as part of a bibliographic title.</desc>
    <content xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><ref name="model.addressLike"/><ref name="model.addressPart"/><ref name="model.annotLike"/><ref name="model.biblLike"/><ref name="model.dateLike"/><ref name="model.editorialLike"/><ref name="model.figureLike"/><ref name="model.identifierLike"/><ref name="model.lbLike"/><ref name="model.locrefLike"/><ref name="model.measurementLike"/><ref name="model.nameLike"/><ref name="model.nameLike.label"/><ref name="model.pbLike"/><ref name="model.rendLike"/><ref name="symbol"/></choice></zeroOrMore>
    </content>
  </macroSpec><macroSpec rend="add" ident="data.BETYPE" module="MEI.shared" type="dt">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">Datatypes for values in begin, end, abstype and inttype attributes.</desc>
    <content xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <valList type="closed">
        <valItem ident="byte">
          <desc>Bytes.</desc>
        </valItem>
        <valItem ident="smil">
          <desc>Synchronized Multimedia Integration Language.</desc>
        </valItem>
        <valItem ident="midi">
          <desc>MIDI clicks.</desc>
        </valItem>
        <valItem ident="mmc">
          <desc>MIDI machine code.</desc>
        </valItem>
        <valItem ident="mtc">
          <desc>MIDI time code.</desc>
        </valItem>
        <valItem ident="smpte-25">
          <desc>SMPTE 25 EBU.</desc>
        </valItem>
        <valItem ident="smpte-24">
          <desc>SMPTE 24 Film Sync.</desc>
        </valItem>
        <valItem ident="smpte-df30">
          <desc>SMPTE 30 Drop.</desc>
        </valItem>
        <valItem ident="smpte-ndf30">
          <desc>SMPTE 30 Non-Drop.</desc>
        </valItem>
        <valItem ident="smpte-df29.97">
          <desc>SMPTE 29.97 Drop.</desc>
        </valItem>
        <valItem ident="smpte-ndf29.97">
          <desc>SMPTE 29.97 Non-Drop.</desc>
        </valItem>
        <valItem ident="tcf">
          <desc>AES Time-code character format.</desc>
        </valItem>
        <valItem ident="time">
          <desc>ISO 24-hour time format: HH:MM:SS.ss.</desc>
        </valItem>
      </valList>
    </content>
  </macroSpec><classSpec rend="add" ident="att.accid.log" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
