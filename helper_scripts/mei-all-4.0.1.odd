        <valList type="semi">
          <valItem ident="paper">
            <desc>Paper.</desc>
          </valItem>
          <valItem ident="parch">
            <desc>Parchment.</desc>
          </valItem>
          <valItem ident="mixed">
            <desc>Mixed materials.</desc>
