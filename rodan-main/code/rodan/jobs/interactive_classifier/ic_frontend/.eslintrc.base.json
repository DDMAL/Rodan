{
    "root": true,
    "extends": "eslint:recommended",

    "rules": {
        "camelcase": 0,
        "no-bitwise": 2,
        "eqeqeq": 2,
        "no-console": 0,
        "guard-for-in": 2,
        "no-extend-native": 2,
        "no-use-before-define": [2, {"functions": false, "classes": false}],
        "no-unused-vars": 2
    }
}
